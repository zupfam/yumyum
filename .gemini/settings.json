{
  "context": {
    "fileName": "AGENTS.md",
    "includeDirectories": [".bmad-core"]
  },
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp", "--api-key", "${CTX7_API_KEY}"]
    }
  },
  // üêõ CORRECTION APPLIED HERE
  "model": {
    "name": "gemini-2.5-pro"
  },
  // -------------------------
  "ui": {
    "accessibility": {
      "screenReader": false
    },
    "showStatusInTitle": true,
    "showMemoryUsage": true,
    "showLineNumbers": true,
    "showCitations": true,
    "theme": "Default"
  },
  "tools": {
    "shell": {
      "showColor": true,
      "enableInteractiveShell": true
    },
    "truncateToolOutputThreshold": 2000000,
    "autoAccept": true
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  },
  "general": {
    "preferredEditor": "code",
    "enablePromptCompletion": false,
    "debugKeystrokeLogging": false
  },
  "output": {
    "format": "text"
  },
  "ide": {
    "enabled": true
  }
}
